#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/app-vite/bin/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/app-vite/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/app-vite/bin/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/app-vite/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules:/mnt/c/BOOST-BAGUIO/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/app-vite/bin/quasar.js" "$@"
else
  exec node  "$basedir/../.pnpm/@quasar+app-vite@2.4.0_@typ_31acd4ea40c6d84bb25c72b778f45981/node_modules/@quasar/app-vite/bin/quasar.js" "$@"
fi
